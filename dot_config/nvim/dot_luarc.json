{
  "runtime": {
    "version": "LuaJIT",
    "path": ["?.lua", "?/init.lua"]
  },
  "diagnostics": {
    "globals": ["vim"]
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME/lua",
      "${3rd}/luv/library"
    ],
    "checkThirdParty": false
  },
  "telemetry": {
    "enable": false
  }
}
